{
	"cniVersion": "0.3.0",
	"name": "azure",
	"plugins": [
		{
			"type": "azure-vnet",
			"mode": "bridge",
			"bridge": "azure0",
			"capabilities": {
				"dns": true,
				"portMappings": true
			},
			"executionMode": "v4swift",
			"ipam": {
				"type": "azure-cns"
			},
			"dns": {
				"nameservers": [
					"10.0.0.10",
					"168.63.129.16"
				],
				"search": [
					"svc.cluster.local"
				]
			},
			"runtimeConfig": {
				"dns": {}
			},
			"windowsSettings": {
				"hnsTimeoutDurationInSeconds": 120
			},
			"AdditionalArgs": [
				{
					"name": "EndpointPolicy",
					"value": {
						"Type": "OutBoundNAT",
						"ExceptionList": [
							"10.240.0.0/16",
							"10.0.0.0/8"
						]
					}
				},
				{
					"name": "EndpointPolicy",
					"value": {
						"Type": "ROUTE",
						"DestinationPrefix": "10.0.0.0/8",
						"NeedEncap": true
					}
				}
			]
		}
	]
}
